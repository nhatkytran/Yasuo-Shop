  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --inspect --respawn --transpile-only src/server.ts",
    "dev-data": "ts-node-dev --inspect --transpile-only dev-data/import-dev-data",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
    "config": "^3.3.11",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "hpp": "^0.2.3",
    "mongoose": "^8.3.1",
    "morgan": "^1.10.0",
    "pino": "^8.20.0",
    "pino-pretty": "^11.0.0"
  },
  "devDependencies": {
    "@types/config": "^3.3.4",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/morgan": "^1.9.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
